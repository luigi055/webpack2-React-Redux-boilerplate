{
  "name": "react-redux-webpack2-boilerplate",
  "version": "1.0.0",
  "description": "Webpack 2 Boilerplate for React + Redux",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "webpack-dashboard -- node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "prod": "export NODE_ENV=production || SET \"NODE_ENV=production\" && webpack-dashboard -- node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js -p",
    "build-dev": "rm -rf ./public && webpack",
    "build-dev--watch": "webpack -d -w",
    "build": "export NODE_ENV=production || SET \"NODE_ENV=production\" && rm -rf ./public && webpack -p",
    "test": "mocha --compilers js:babel-core/register --require ./src/tests/testHelpers.js --recursive ./src/tests",
    "test-watch": "yarn run test -- --watch",
    "clear": "rm -rf ./public",
    "flow init": "flow init",
    "flow start": "flow start",
    "flow stop": "flow stop",
    "flow status": "flow status",
    "flow coverage": "flow coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/luigi055/webpack2-React-Redux-boilerplate.git"
  },
  "keywords": [
    "react",
    "redux",
    "react-router-v4",
    "webpack2",
    "boilerplate"
  ],
  "author": "Pedro La Rosa",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "7.1.1",
    "babel-core": "^6.24.0",
    "babel-loader": "7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "chai": "4.0.2",
    "chai-jquery": "^2.0.0",
    "css-loader": "0.28.4",
    "css-mqpacker": "6.0.1",
    "eslint": "4.0.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "0.11.2",
    "flow-bin": "0.48.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.2.0",
    "jsdom": "11.0.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "pixrem": "^3.0.2",
    "postcss-loader": "2.0.6",
    "postcss-vmin": "3.0.0",
    "postcss-will-change": "2.0.0",
    "react-addons-test-utils": "15.6.0",
    "resolve-url-loader": "^2.0.0",
    "sass-loader": "6.0.6",
    "script-loader": "^0.7.0",
    "sinon": "2.3.4",
    "sinon-chai": "2.11.0",
    "style-loader": "0.18.2",
    "stylelint": "^7.9.0",
    "stylelint-config-standard": "^16.0.0",
    "url-loader": "0.5.9",
    "webpack": "^2.2.1",
    "webpack-dashboard": "0.4.0",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "express": "^4.15.2",
    "font-awesome": "^4.7.0",
    "foundation-sites": "^6.3.0",
    "jquery": "3.2.1",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0"
  }
}
